fn recur_1 num (num n) start 
    if n == 1 start 
        return 3
    end 
    return -2 + recur_1(n-1)
end
fn recur_2 num (num n) start 
    if n == 1 start 
        return -1
    end 
    return 2 * recur_2(n-1)
end
fn main none () start 
    num n 
    in n 
    out recur_1(n)
    out recur_2(n)
end

