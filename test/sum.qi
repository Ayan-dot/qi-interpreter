fn sum num (arr x, num size) start 
    num sum1
    sum1 = 0
    for i of range(0, size) start 
        sum1+=x.at(i)
    end
    return sum1
end
fn rec_sum num (arr x, num size, num curr) start 
    if curr == size start
        return 0
    end 
    return x.at(curr) + rec_sum(x, size, curr+1)
end
fn main none () start 
    num size
    in size
    arr arr1
    num p
    for i of range(0, size) start 
        in p 
        arr1.push(p)
    end
    
end